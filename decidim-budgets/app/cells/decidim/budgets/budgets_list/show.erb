<% if (budgets - voted).any? %>
  <div class="section" id="budgets-to-vote">
    <h2 class="h5 md:h3 decorator mb-8">
      <%= t(:my_budgets, scope: i18n_scope) %>
    </h2>

<%= render :card_list %>

<% if !voting_finished? && voted? %>
  <div class="section">
    <h2 class="h5 md:h3 decorator mb-8">
      <%= t(:voted_budgets, scope: i18n_scope) %>
    </h2>

    <%= render :voted %>
    <% end %>
  </div>
</div>
